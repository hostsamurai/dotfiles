defaults.pcm.rate_converter "speexrate_best"

pcm.!default iec958

pcm.dmixer {
  type dmix
  ipc_key 1024
  ipc_key_add_uid 1
  ipc_perm 0660
  slave {
    pcm "hw:0,1"
    period_size 9216
    buffer_size 262144
    rate 192000
  }
}

pcm.nvidiahdmi {
  type hw
  card 2
  device 3
}

ctl.nvidiahdmi {
  type plug
  slave.pcm "nvidiahdmi"
}

pcm.dp {
  type hw
  card 2
  device 7
}

ctl.dp {
  type plug
  slave.pcm "dp"
}

